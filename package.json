{
  "name": "calendar",
  "description": "Calendar app",
  "author": "Dmitriy Shved <dshved@hotmail.com>",
  "version": "0.1.0",
  "homepage": "https://frontendcalendar.tk",
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.0",
    "react-scripts": "2.1.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-preloader-icon": "^0.1.1",
    "react-select": "^1.2.1",
    "styled-components": "^3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "lint": "eslint ./src",
    "format": "prettier-eslint --trailing-comma all --no-semi --single-quote --print-width 120 --jsx-bracket-same-line --write 'package.json' 'src/**/*.js'"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "format",
      "eslint",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/dshved/frontendcalendar/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dshved/frontendcalendar.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
